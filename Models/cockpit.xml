 <!-- animation -->

 <animation>
  <type>select</type>
  <object-name>Copilot_inst</object-name>
  <condition>
   <property>/sim/crashed</property>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>Copilot</object-name>
  <condition>
   <not>
    <property>/sim/crashed</property>
   </not>
  </condition>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Horn</object-name>
  <object-name>Steuer_l</object-name>
  <object-name>Steuer_r</object-name>
  <property>/controls/flight/elevator</property>
  <factor>-8</factor>
  <center>
   <x-m>-2.92</x-m>
   <y-m>0.0</y-m>
   <z-m>1.40</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Steuer_l</object-name>
  <property>controls/flight/aileron</property>
  <factor> -90.000 </factor>
  <offset-deg> 0.000 </offset-deg>
  <center>
   <x-m>-2.86</x-m>
   <y-m>-1.26</y-m>
   <z-m>2.1</z-m>
  </center>
  <axis>
    <x> 1.0 </x>
    <y> 0.0 </y>
    <z> 0.0 </z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Steuer_r</object-name>
  <property>controls/flight/aileron</property>
  <factor> -90.000 </factor>
  <offset-deg> 0.000 </offset-deg>
  <center>
   <x-m>-2.86</x-m>
   <y-m>-0.46</y-m>
   <z-m>2.1</z-m>
  </center>
  <axis>
    <x> 1.0 </x>
    <y> 0.0 </y>
    <z> 0.0 </z>
  </axis>
 </animation>

