<?xml version="1.0"?>

<PropertyList>

 <path>Kompass.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>Kugel</object-name>
  <property>/instrumentation/magnetic-compass/indicated-heading-deg</property>
  <center>
   <x-m>-0.01</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

</PropertyList>
