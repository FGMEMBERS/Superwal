<?xml version="1.0"?>
<PropertyList>

 <path>Laengsneigung.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>Ring1</object-name>
  <property>/orientation/pitch-deg</property>
  <interpolation>
   <entry><ind>-20</ind><dep>-135</dep></entry>     
   <entry><ind>-10</ind><dep> -65</dep></entry>     
   <entry><ind>  0</ind><dep>   0</dep></entry>     
   <entry><ind> 10</ind><dep>  90</dep></entry>     
   <entry><ind> 15</ind><dep> 120</dep></entry>     
  </interpolation>
  <center>
   <x-m>-0.01</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>-1.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Ring2</object-name>
  <property>/orientation/pitch-deg</property>
  <interpolation>
   <entry><ind>-20</ind><dep> 135</dep></entry>     
   <entry><ind>-10</ind><dep>  65</dep></entry>     
   <entry><ind>  0</ind><dep>   0</dep></entry>     
   <entry><ind> 10</ind><dep> -90</dep></entry>     
   <entry><ind> 15</ind><dep>-120</dep></entry>     
  </interpolation>
  <center>
   <x-m>-0.01</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>-1.0</z>
  </axis>
 </animation>

 
</PropertyList>