<?xml version="1.0"?>

<PropertyList>

 <path>Kurszeiger.ac</path>

 <animation>
   <type>translate</type>
   <object-name>Zeiger</object-name>
   <property>/instrumentation/adf/indicated-bearing-deg</property>
   <interpolation>
     <entry><ind> -80</ind><dep>-0.04</dep></entry>
     <entry><ind> -20</ind><dep>-0.02</dep></entry>
     <entry><ind> 0</ind><dep>  0</dep></entry>
     <entry><ind> 20</ind><dep> 0.02</dep></entry>
     <entry><ind> 80</ind><dep> 0.04</dep></entry>
     <entry><ind>80.1</ind><dep>-0.04</dep></entry>
     <entry><ind>280</ind><dep>-0.04</dep></entry>
     <entry><ind>340</ind><dep>-0.02</dep></entry>
     <entry><ind>360</ind><dep> 0</dep></entry>

   </interpolation>
   <axis>
     <x>0</x>
     <y>1</y>
     <z>0</z>
   </axis>
 </animation>

</PropertyList>

