<?xml version="1.0"?>

<PropertyList>

 <path>Borduhr.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>Minute</object-name>
  <property>/instrumentation/clock/indicated-min</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>   0</dep></entry>     
   <entry><ind> 60.0</ind><dep>360</dep></entry>     
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Stunde</object-name>
  <property>/instrumentation/clock/indicated-hour</property>
  <interpolation>
   <entry><ind>  0</ind><dep>  0</dep></entry>     
   <entry><ind> 12</ind><dep>360</dep></entry>     
   <entry><ind> 13</ind><dep> 30</dep></entry>     
   <entry><ind> 24</ind><dep>360</dep></entry>     
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Stunde</object-name>
  <property>/instrumentation/clock/indicated-min</property>
  <factor> 0.5</factor>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


